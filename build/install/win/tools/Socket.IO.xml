<service>
	<id>ASC.Socket.IO</id>
	<name>ASC.Socket.IO</name>
	<description>This service runs ASC.Socket.IO</description>
	<priority>RealTime</priority>
	<startmode>Automatic</startmode>
	<onfailure action="restart" delay="1 sec"/>
	<executable>&quot;{APPDIR}services\ASC.Socket.IO\service\server.js&quot;</executable>
	<workingdirectory>{APPDIR}services\ASC.Socket.IO\service</workingdirectory>
	<log mode="roll-by-size">
  		<sizeThreshold>10240</sizeThreshold>
  		<keepFiles>8</keepFiles>
	</log>
</service>